package Enc1 {
    params {
        n: Integer,
        m: Integer,
        p: Integer,
        encn: fn Bits(n),Bits(n),Bits(n) -> Bits(m),
        encm: fn Bits(n),Bits(m),Bits(n) -> Bits(p),
        zeron: Bits(n),
        zerom: Bits(m),
    }

   import oracles {
        GETKEYSIN(h: Integer) -> Table(Bool,Bits(n)),
        GETBIT(h: Integer) -> Bool,
    }

    state {
    }

    oracle ENCN(j: Integer,d: Bool, mzero: Bits(n), mone: Bits(n)) -> Bits(m) {
        K <- invoke GETKEYSIN(j);
        z <- invoke GETBIT(j);
        r <-$ Bits(n);
        if (b==z)
        {c <- encn(Unwrap(K[z]),mzero,r);}
        else
        {c <- encn(Unwrap(K[z]),mone,r);}
        return c;
    }

    oracle ENCM(j: Integer,d: Bool, mzero: Bits(m), mone: Bits(m)) -> Bits(p) {
        K <- invoke GETKEYSIN(j);
        z <- invoke GETBIT(j);
        r <-$ Bits(n);
        if (b==z)
        {c <- encm(Unwrap(K[z]),mzero,r);}
        else
        {c <- encm(Unwrap(K[z]),mone,r);}
        return c;
    }

}