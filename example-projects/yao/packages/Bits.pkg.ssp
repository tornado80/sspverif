package Bits {
    state {
        z: Table(Integer,Bool),
    }

    oracle GETBIT(h: Integer) -> Bool {
        assert (z[h] != None as Bool);
        zz <- Unwrap(z[h]);
        return zz;
    }

    oracle SETBIT(h: Integer, zz:Bool) -> () {
        assert (z[h] == None as Bool);
        z[h] <- Some(zz);
        return ;
    }

}