package Key {
    params {
        b: Bool
    }

    state {
        k: Maybe(Bits(256))
    }

    oracle SET(kp: Bits(256)) {
        assert (k == None);
        if b {
            k1 <-$ Bits(256);
            k <- Some(k1);
        } else {
            k <- Some(kp);
        }
    }

    oracle GET() -> Bits(256) {
        assert (k != None as Bits(256));
        return Unwrap(k);
    }
}